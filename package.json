{
  "name": "serverless-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.5.0",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-animate-on-scroll": "^2.1.2",
    "@types/react-dom": "^16.9.0",
    "animate.css": "^4.1.0",
    "classnames": "^2.2.6",
    "query-string": "^6.13.0",
    "react": "^16.13.0",
    "react-animate-on-scroll": "^2.1.5",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "NODE_ENV=production PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn test && yarn build",
    "deploy": "serverless",
    "destroy": "serverless remove",
    "test-broken": "react-scripts test",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --silent --watchAll=false",
    "test:watch": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.10.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.0.0",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/react-router-dom": "^5.1.3",
    "cypress": "^4.7.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^4.13.1",
    "react-test-renderer": "^16.13.1",
    "serverless": "^1.66.0",
    "ts-jest": "^26.1.0"
  },
  "jest2": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
